<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Doug Ware's string extension methods.</Title>
    <Authors>DougWare</Authors>
    <Description>Simple, but useful methods for extracting strings based on a starting and ending string.</Description>
    <PackageProjectUrl>https://github.com/douglasware/ElumenotionSite</PackageProjectUrl>
    <PackageIcon>logo.png</PackageIcon>
    <RepositoryUrl>https://github.com/douglasware/ElumenotionSite</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
